import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner Scan = new Scanner(System.in);
     int N = Scan.nextInt();
     for(int i=1;i<=N;i++){
         for(int j=N;j>=i;j--){
             System.out.print("*");
         }
           System.out.println();
     }
    }
}
